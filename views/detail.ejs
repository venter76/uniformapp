<header>

  <nav class="navbar">
      <!-- fixed-top bg-body-tertiary"> -->
         <div class="container-fluid">
           
           <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
             <span class="navbar-toggler-icon"></span>
           </button>
           <div class="collapse navbar-collapse" id="navbarNav">
             <ul class="navbar-nav">
                 <li class="nav-item">
                 
                     <form action="/index" method="get">
                       <button type="submit" class="nav-link btn btn-link text-white">Home</button>
                     </form>
                   </li>
 
              
               <!-- <li class="nav-item">
                <form action="/rosterset" method="get">
                  <button type="submit" class="nav-link btn btn-link text-white">Roster</button>
                </form>
              </li> -->
             </ul>
           </div>
         </div>
       </nav> 
  </header>



  <body class="bg-dark">

    <div class="container">
     
      <h1 style="padding-left: 60px; color: white;"><%= itemName %></h1>
      <br>
      <div class="row mb-4">
          <div class="col-12">
              <div class="card mb-4">
                  <div class="card-body">
                      <form method="POST" action="/detailmove"> <!-- Change the action URL to your desired endpoint -->
                          <h5 class="card-title" style="color: white;">Pack:</h5>
                          <select class="form-control" name="pack">
                              <option value="" disabled selected>Choose number</option>
                              <option value="1">1</option>
                              <option value="2">2</option>
                              <option value="3">3</option>
                              <option value="4">4</option>
                              <option value="5">5</option>
                          </select>
  
                          <br>
  
                          <h5 class="card-title" style="color: white;">Wear:</h5>
                          <select class="form-control" name="wear">
                              <option value="" disabled selected>Choose number</option>
                              <option value="1">1</option>
                              <option value="2">2</option>
                              <option value="3">3</option>
                              <option value="4">4</option>
                              <option value="5">5</option>
                          </select>
                          <br>
  
                          <input type="hidden" name="itemName" value="<%= itemName %>">
                          <button type="submit" class="btn btn-primary btn-sm">Submit</button>
                      </form>
                  </div>
              </div>
          </div>
      </div>
  </div>
  
   
    
       
            <br>
           
                  
               
                <br>
                
                <div class="row mb-4">
                  <div class="col-12">
                    <div class="card mb-4">
                      <div class="card-body">
                        <h5 class="card-title" style="color: white;">Uniform numbers:</h5>
                        <br>
                        <div>
                          <p><strong style="color: white;">Total:</strong> <span style="color: white;"><%= total %></span></p>
                          <p><strong style="color: white;">Ste/Pet:</strong> <span style="color: white;"><%= stePet %></span></p>
                          <p><strong style="color: white;">Che:</strong> <span style="color: white;"><%= che %></span></p>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
                
    

    

          
               
            


    <!-- <script src="index.js"></script> -->
   <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js" integrity="sha384-geWF76RCwLtnZ8qwWowPQNguL3RmwHVBC9FhGdlKrxdiJJigb/j/68SIy3Te4Bkz" crossorigin="anonymous">
        </script>
       <script>
        if ('serviceWorker' in navigator) {
          navigator.serviceWorker.register('/service-worker.js')
          .then(function(registration) {
            console.log('Service Worker registered with scope:', registration.scope);
          })
          .catch(function(error) {
            console.error('Service Worker registration failed:', error);
          });
        }
        </script>


</script>

        
</body>

